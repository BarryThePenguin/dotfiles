[user]
	name = Jonathan Haines
	email = jonno.haines@gmail.com
	signingkey = ssh-rsa 

[core]
	excludesfile = /Users/jonno/.gitignore_global

[github]
  	user = barrythepenguin

[hub]
	protocol = ssh:

[init]
	defaultBranch = main

[gpg]
	format = ssh

[gpg "ssh"]
  	program = "/Applications/1Password.app/Contents/MacOS/op-ssh-sign"
	allowedSignersFile = /Users/jonno/.ssh/allowed_signers

[merge]
	conflictstyle = zdiff3

[push]
	default = simple

[pull]
	rebase = true

[rebase]
	autoStash = true

[url "git@github.com:blake-education/"]
	insteadOf = https://github.com/blake-education/

[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
